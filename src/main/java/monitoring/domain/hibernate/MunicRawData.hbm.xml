<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="monitoring.domain">
	<class name="MunicRawData" table="municrawdata">
		<id name="id">
			<generator class="identity" />
		</id>
		<property name="arrived" not-null="true"></property>
		<property name="processed" not-null="true"></property>
		<property name="rawData" length="20000" not-null="true"></property>
	</class>
</hibernate-mapping>