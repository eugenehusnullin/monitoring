<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="monitoring.domain">
	<class name="TopMapzonePoint" table="top_mapzone_point">
		<composite-id>
<!-- 			<key-property name="id" column="mapzoneId"></key-property> -->
			<key-many-to-one name="mapzone" column="mapzoneId"></key-many-to-one>
			<key-property name="sort" column="sort"></key-property>
		</composite-id>
		<property name="lat" column="lat"></property>
		<property name="lng" column="lng"></property>
	</class>
</hibernate-mapping>