<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="monitoring.handler.accelerometer">
	<class name="Accelerometr" table="accelerometr">
		<id name="id">
			<generator class="identity" />
		</id>
		<property name="terminalId"></property>
		<property name="date"></property>
		<property name="accX" column="accx"></property>
		<property name="accY" column="accy"></property>
		<property name="accZ" column="accz"></property>
	</class>
</hibernate-mapping>