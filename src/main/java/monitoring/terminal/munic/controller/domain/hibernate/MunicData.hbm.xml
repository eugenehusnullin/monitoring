<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="monitoring.domain">
	<class name="MunicData" table="municdata">
		<composite-id>
			<key-many-to-one name="municItemRawData" column="itemrawdataid"></key-many-to-one>
		</composite-id>
		<one-to-one name="municItemRawData" ></one-to-one>
		<property name="event" not-null="true" length="24"></property>
		<property name="asset" not-null="true" index="municdata_idx1"></property>
		<property name="recordedAt" index="municdata_idx2"></property>
		<property name="lat"></property>
		<property name="lon"></property>
		<property name="gprmcValid" length="1"></property>
		<property name="gpsSpeed"></property>
		<property name="gpsDir"></property>
		<property name="dioIgnition"></property>
		<property name="odoFull"></property>
		<property name="dioAlarm"></property>
		<property name="driverId" length="15"></property>
		<property name="dioInTor"></property>
		<property name="batt"></property>
		<property name="gprsHeader"></property>
		<property name="rssi"></property>
		<property name="battVolt"></property>
		<property name="mvtState"></property>
		<one-to-one name="municDataBehave" cascade="all"></one-to-one>
		<one-to-one name="municDataMdi" cascade="all"></one-to-one>
		<property name="topAutoId"></property>
		<property name="mapzoneId"></property>
		<property name="mapzoneType"></property>
	</class>
</hibernate-mapping>