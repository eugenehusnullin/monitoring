<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="monitoring.domain">
	<class name="MunicItemRawData" table="municitemrawdata">
		<id name="id">
			<generator class="identity" />
		</id>
		<many-to-one name="municRawData" column="municrawdataid"></many-to-one>
		<property name="processed" not-null="true"></property>
		<property name="wialonSended" not-null="true"></property>
		<property name="itemRawData" length="2024" not-null="true"></property>
	</class>
</hibernate-mapping>