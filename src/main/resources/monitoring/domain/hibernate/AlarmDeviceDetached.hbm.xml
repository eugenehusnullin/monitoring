<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="monitoring.domain">
	<class name="AlarmDeviceDetached" table="terminal_detach">
		<id name="id">
			<generator class="identity" />
		</id>
		<property name="terminalId"></property>
		<property name="carId" column="autoId"></property>
		<property name="eventDate"></property>
	</class>
</hibernate-mapping>