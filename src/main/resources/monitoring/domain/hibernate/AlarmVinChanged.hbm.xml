<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="monitoring.domain">
	<class name="AlarmVinChanged" table="top_auto_vin_change">
		<id name="carId" column="autoId"></id>
		<property name="vinNew" column="VINnew"></property>
		<property name="vinOld" column="VINold"></property>
		<property name="alarmDate" column="datetime"></property>
	</class>
</hibernate-mapping>