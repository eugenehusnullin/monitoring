<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
		"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="monitoring.handler.position">
	<class name="Position" table="position">
		<id name="id">
			<generator class="identity" />
		</id>
		<property name="terminalId"></property>
		<property name="date"></property>
		<property name="lat"></property>
		<property name="lon"></property>
		<property name="speed"></property>
		<property name="satellites"></property>
		<property name="accuracy"></property>
		<property name="course"></property>
		<property name="altitude"></property>
		<property name="gpsValid"></property>
		<property name="abnormal"></property>
	</class>
</hibernate-mapping>